{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "cypress"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint", "cypress"],
  "rules": {
    "quotes": ["error", "double"]
  },
  "ignorePatterns": ["**.cy.ts", "**/cypress/*.ts"]
}
